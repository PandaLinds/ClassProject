INCLUDE_DIRS = 
LIB_DIRS = 
CC=g++

CDEFS=
CFLAGS= -Wall $(INCLUDE_DIRS) $(CDEFS)
LIBS=

HFILES=  map.hpp
CFILES= map.cpp maptest.cpp

SRCS= ${HFILES} ${CFILES}
OBJS= ${CFILES:.cpp=.o}

all:	maptest 

clean:
	-rm -f *.o *.d 
	-rm -f maptest

distclean:
	-rm -f *.o *.d

maptest:	${OBJS}
	$(CC) $(LDFLAGS) $(CFLAGS) -o $@ $(OBJS) $(LIBS)


depend:

.cpp.o:
	$(CC) $(CFLAGS) -c $<
